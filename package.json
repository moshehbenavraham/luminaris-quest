{
  "name": "luminaris-quest",
  "private": true,
  "version": "0.2.1",
  "type": "module",
  "description": "Therapeutic AI-powered RPG adventure. Built with AI. See README.md for development standards.",
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.app.json --noEmit && vite build",
    "build:dev": "vite build --mode development",
    "build:deploy": "npm ci --include=optional && tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 250",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "optimize-images": "node scripts/optimize-images.js",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:full": "lhci autorun",
    "prepare": "husky"
  },
  "dependencies": {
